@{
    ViewData["Title"] = "Docentes";
}

<div class="container my-5">
    <div class="d-flex justify-content-between align-items-center mb-4">
        <h2 class="fw-bold p-2">Lista de docentes</h2>
    </div>

    <table class="table">
        <thead>
        <tr>
            <th>Nombre</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var docente in Model)
        {
        <tr>
            <td>@docente.NombreDocente</td>
        </tr>
        }
        </tbody>
    </table>
</div>